<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.enn.iot</groupId>
  <artifactId>gateway-std-client</artifactId>
  <name>gateway-std-client</name>
  <version>1.0.0-SNAPSHOT</version>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>3.0.0</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <artifactSet>
                <includes>
                  <include>org.springframework.cloud:spring-cloud-openfeign-core</include>
                  <include>org.springframework:spring-web</include>
                </includes>
              </artifactSet>
              <filters>
                <filter>
                  <artifact>org.springframework.cloud:spring-cloud-openfeign-core</artifact>
                  <includes>
                    <include>org/springframework/cloud/openfeign/EnableFeignClients.class</include>
                    <include>org/springframework/cloud/openfeign/FeignClient.class</include>
                    <include>org/springframework/cloud/openfeign/SpringQueryMap.class</include>
                  </includes>
                </filter>
                <filter>
                  <artifact>org.springframework:spring-web</artifact>
                  <includes>
                    <include>org/springframework/web/bind/annotation/**</include>
                  </includes>
                </filter>
                <filter>
                  <artifact>*:*</artifact>
                  <excludes>
                    <exclude>META-INF/**</exclude>
                  </excludes>
                </filter>
              </filters>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  <repositories>
    <repository>
      <id>maven-releases</id>
      <url>http://nexus.enncloud.cn/repository/releases/</url>
    </repository>
    <repository>
      <id>maven-snapshots</id>
      <url>http://nexus.enncloud.cn/repository/snapshots/</url>
    </repository>
  </repositories>
  <dependencies>
    <dependency>
      <groupId>org.projectlombok</groupId>
      <artifactId>lombok</artifactId>
      <version>1.18.24</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>org.springframework.cloud</groupId>
      <artifactId>spring-cloud-openfeign-core</artifactId>
      <version>3.1.5</version>
      <scope>provided</scope>
      <exclusions>
        <exclusion>
          <artifactId>logback-classic</artifactId>
          <groupId>ch.qos.logback</groupId>
        </exclusion>
        <exclusion>
          <artifactId>logback-core</artifactId>
          <groupId>ch.qos.logback</groupId>
        </exclusion>
        <exclusion>
          <artifactId>slf4j-api</artifactId>
          <groupId>org.slf4j</groupId>
        </exclusion>
        <exclusion>
          <artifactId>spring-boot</artifactId>
          <groupId>org.springframework.boot</groupId>
        </exclusion>
        <exclusion>
          <artifactId>spring-aop</artifactId>
          <groupId>org.springframework</groupId>
        </exclusion>
        <exclusion>
          <artifactId>snakeyaml</artifactId>
          <groupId>org.yaml</groupId>
        </exclusion>
        <exclusion>
          <artifactId>spring-boot-starter-aop</artifactId>
          <groupId>org.springframework.boot</groupId>
        </exclusion>
        <exclusion>
          <artifactId>spring-boot-autoconfigure</artifactId>
          <groupId>org.springframework.boot</groupId>
        </exclusion>
        <exclusion>
          <artifactId>feign-form-spring</artifactId>
          <groupId>io.github.openfeign.form</groupId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>org.springframework</groupId>
      <artifactId>spring-web</artifactId>
      <version>5.3.24</version>
      <scope>provided</scope>
      <exclusions>
        <exclusion>
          <artifactId>spring-beans</artifactId>
          <groupId>org.springframework</groupId>
        </exclusion>
        <exclusion>
          <artifactId>spring-core</artifactId>
          <groupId>org.springframework</groupId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>org.springframework.boot</groupId>
      <artifactId>spring-boot-starter</artifactId>
      <version>2.7.7</version>
      <scope>provided</scope>
      <exclusions>
        <exclusion>
          <artifactId>spring-boot-starter-logging</artifactId>
          <groupId>org.springframework.boot</groupId>
        </exclusion>
        <exclusion>
          <artifactId>snakeyaml</artifactId>
          <groupId>org.yaml</groupId>
        </exclusion>
        <exclusion>
          <artifactId>spring-boot</artifactId>
          <groupId>org.springframework.boot</groupId>
        </exclusion>
        <exclusion>
          <artifactId>spring-boot-autoconfigure</artifactId>
          <groupId>org.springframework.boot</groupId>
        </exclusion>
        <exclusion>
          <artifactId>jakarta.annotation-api</artifactId>
          <groupId>jakarta.annotation</groupId>
        </exclusion>
        <exclusion>
          <artifactId>spring-core</artifactId>
          <groupId>org.springframework</groupId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>com.alibaba</groupId>
      <artifactId>fastjson</artifactId>
      <version>2.0.38</version>
      <scope>provided</scope>
      <exclusions>
        <exclusion>
          <artifactId>fastjson2-extension</artifactId>
          <groupId>com.alibaba.fastjson2</groupId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>com.google.code.gson</groupId>
      <artifactId>gson</artifactId>
      <version>2.10.1</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>top.kdla.framework</groupId>
      <artifactId>kdla-component-model</artifactId>
      <version>1.0.0-SNAPSHOT</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>com.ctg.ag.sdk.biz</groupId>
      <artifactId>sdkpackage</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>${pom.basedir}/lib/ag-sdk-biz-289627.tar.gz-20231204.135557-SNAPSHOT.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.ctg.ag.sdk.core</groupId>
      <artifactId>sdkcore</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>${pom.basedir}/lib/ctg-ag-sdk-core-2.8.0-20230508.100604-1.jar</systemPath>
    </dependency>
  </dependencies>
  <distributionManagement>
    <repository>
      <id>maven-releases</id>
      <url>http://nexus.enncloud.cn/repository/releases/</url>
    </repository>
    <snapshotRepository>
      <id>maven-snapshots</id>
      <url>http://nexus.enncloud.cn/repository/snapshots/</url>
    </snapshotRepository>
  </distributionManagement>
  <properties>
    <kdla.version>1.0.0-SNAPSHOT</kdla.version>
    <lombok.version>1.18.24</lombok.version>
    <java.version>1.8</java.version>
    <gson.version>2.10.1</gson.version>
    <maven.compiler.target>1.8</maven.compiler.target>
    <spring-cloud-openfeign.version>3.1.5</spring-cloud-openfeign.version>
    <spring-web.version>5.3.24</spring-web.version>
    <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <spring.boot.version>2.7.7</spring.boot.version>
    <maven.compiler.source>1.8</maven.compiler.source>
    <fastjson.version>2.0.38</fastjson.version>
  </properties>
</project>

